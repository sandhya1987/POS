<Window x:Class="InvoicePOS.Views.PrinterSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Printing="clr-namespace:System.Drawing.Printing;assembly=System.Drawing"
        Title="PrinterSettings" Height="300" Width="300">
    <Grid>
        <Label Content="Choose Printer:"  Margin="0,80,0,0"></Label>
        <ComboBox Name="com_Printers" Height="30" Width="200" SelectionChanged="OnSelectionChange"></ComboBox>       
    </Grid>
</Window>
