<Window x:Class="InvoicePOS.Views.Main.demopage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InvoicePOS.Views.Main"
        mc:Ignorable="d"
        Title="demopage" Height="300" Width="300">
    <Grid>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Margin="44,64,0,0" VerticalAlignment="Top" Height="115" Width="203">
            <DataGrid.Columns>


                <DataGridTextColumn    Header="BarCode" Binding="{Binding BARCODE}" Width="100" />
                <DataGridTextColumn    Header="Item Name" Binding="{Binding ITEM_NAME}" Width="100"/>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
