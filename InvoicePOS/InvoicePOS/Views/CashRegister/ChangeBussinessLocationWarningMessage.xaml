<Window x:Class="InvoicePOS.Views.CashRegister.ChangeBussinessLocationWarningMessage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InvoicePOS.Views.CashRegister"
        mc:Ignorable="d"
        Title="ChangeBussinessLocationWarningMessage" Height="165.546" Width="440.336">
    <Grid>
        <Label x:Name="label" Content="There is an invoice i nprogress. Please cancel or hold current invoice." HorizontalAlignment="Left" Margin="18,20,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <Button x:Name="button" Content="Hold Invoice" HorizontalAlignment="Left" Margin="58,71,0,0" VerticalAlignment="Top" Width="85" Command="{Binding ChangeBussinessLocationItem}" Click="button_Click"/>
        <Button x:Name="button_Copy" Content="Cancel Invoice" HorizontalAlignment="Left" Margin="171,71,0,0" VerticalAlignment="Top" Width="94" Command="{Binding ChangeBussinessLocationItem}"/>
        <Button x:Name="button_Copy1" Content="Do Nothing" HorizontalAlignment="Left" Margin="289,71,0,0" VerticalAlignment="Top" Width="83"/>

    </Grid>
</Window>
