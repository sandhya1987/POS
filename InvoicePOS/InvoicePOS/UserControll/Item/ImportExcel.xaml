<Window x:Class="InvoicePOS.UserControll.Item.ImportExcel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:p="clr-namespace:InvoicePOS.Properties"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" WindowStartupLocation="CenterScreen"
        Title="{x:Static p:Resources.String764}" Height="186.091" Width="300">
    <Grid>
        <Button Content="{x:Static p:Resources.String765}" Command="{Binding DownloadExcel}" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="272" Height="31"/>
        <Button Content="{x:Static p:Resources.String766}" Command="{Binding ExcelImportClick}" HorizontalAlignment="Left" Margin="10,70,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="90,70,0,0" TextWrapping="Wrap" Text="{Binding ExcelPath}" VerticalAlignment="Top" Width="192"/>
        <Button Content="{x:Static p:Resources.String513}" Command="{Binding InsertToExcel}" HorizontalAlignment="Left" Margin="129,112,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="{x:Static p:Resources.label13}" Command="{Binding Cancel}" HorizontalAlignment="Left" Margin="209,112,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
