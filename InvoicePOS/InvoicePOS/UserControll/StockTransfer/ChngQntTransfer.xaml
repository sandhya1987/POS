<Window x:Class="InvoicePOS.UserControll.StockTransfer.ChngQntTransfer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" WindowStartupLocation="CenterScreen"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ChngQntTransfer" Height="300" Width="300">
    <Window.InputBindings>
        <KeyBinding Key="Enter" Command="{Binding ChngStockTransfer}"/>
        <KeyBinding Key="Escape" Command="{Binding Cancel}"/>
    </Window.InputBindings>
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="13,65,0,0" TextWrapping="Wrap" Text="Transfer Quantity" VerticalAlignment="Top" Height="20"/>
        <Button Content="Save" Command="{Binding Path=ChngStockTransfer}" HorizontalAlignment="Left" Margin="87,123,0,0" VerticalAlignment="Top" Width="57"/>
        <TextBox HorizontalAlignment="Left" Height="23" Text="{Binding TRANS_QUANTITY}" Margin="129,65,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Content="Cancel" Command="{Binding Path=Cancel}" HorizontalAlignment="Left" Margin="155,123,0,0" VerticalAlignment="Top" Width="59"/>

    </Grid>
</Window>
